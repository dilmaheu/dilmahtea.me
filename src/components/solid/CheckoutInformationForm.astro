---
import subset from "@utils/shared/subset";
import notificationIcons from "@utils/notificationIcons";
import CheckoutInformationForm from "@solid/CheckoutInformationForm";

let { recurData } = Astro.props;

recurData = subset(recurData, [
  "text_first_name",
  "first_name_placeholder",
  "text_last_name",
  "last_name_placeholder",
  "text_country",
  "country_placeholder",
  "text_city",
  "city_placeholder",
  "text_street",
  "street_placeholder",
  "text_postal_code",
  "postal_code_placeholder",
  "countries",
  "Label_tag_text",
  "label_auto_tag",
  "Tag_add_text",
  "Tag_add_placeholder",
  "Tag_suggestions",
  "text_more_address",
  "text_hide_more_address",
  "text_select_or_create_tag",
]);
---

<CheckoutInformationForm client:load {recurData} {notificationIcons} />
